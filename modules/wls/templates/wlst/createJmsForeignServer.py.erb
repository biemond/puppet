
wlsUser    = '<%= @wlsUser %>'  
password   = '<%= @password %>' 
machine    = '<%= @address %>'  
portNumber = '<%= @port %>'     

subDeployment             = ''
JNDIInitialContextFactory = ''
JNDIpropertyName          = ''
connectionURL             = ''

       
